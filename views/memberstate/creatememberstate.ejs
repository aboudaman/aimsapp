<!-- Start Bootstrap Form -->
<div class="container">
	<h4 style="margin-top: 25px;"> Register A SADC Member State </h4>
	<div class="row" style="margin-top: 55px;">
		<div class="col">
			<form action="/memberstate/register" method="post" enctype="multipart/form-data">
				<div class="form-group">
					<label for="Member State Name">Enter Member State</label>
					<input type="text" name="countryName" class="form-control" placeholder="Enter Member State Name..." >
				</div>
				<div class="form-group">
					<label for="Population">Enter Population</label>
					<input type="text" name="population" class="form-control" placeholder="Enter Population..." >
				</div>
				<div class="form-group">
					<label for="Land Area">Enter Land Area</label>
					<input type="text" name="area" class="form-control" placeholder="Enter Land Area..." >
				</div>
				<div class="form-group">
					<label for="currency">Enter Currency Used</label>
					<input type="text" name="currency" class="form-control" placeholder="Enter Currency Used..." >
				</div>
				<div class="form-group" style="margin-bottom: 55px;">
					<label for="Country Flag">Upload Flag</label>
					<input type="file" class="form-control-file" name="flag"  placeholder="Upload Member State Flag..." >
				</div>
				<button class="btn btn-primary" type="submit" style="margin-bottom: 25px;">Submit form</button>
				<button class="btn btn-primary" type="cancel" style="margin-bottom: 25px;">Cancel</button>

			  </form>
		</div>
	</div>
</div>

<!-- End Bootstrap Form -->


