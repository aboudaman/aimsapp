<!-- <table id="example" class="display" style="width:100%"> -->
<table id="example" class="display uk-table uk-table-hover">
        <thead>
            <tr>
                <th>Country Name</th>
                <th>Population</th>
	   </tr>
        </thead>
        <tbody>
	<% members.forEach(member => { %>
            <tr>
                <td><%= member.countryName %></td>
                <td><%= member.population %></td>
            </tr>
           <% }) %>
        </tbody>
        <tfoot>
            <tr>
                <th>Country Name</th>
                <th>Population</th>
	   </tr>
        </tfoot>
    </table>
    <script>
	$(document).ready(function() {
	    $('#example').DataTable( {
		dom: 'Bfrtip',
		buttons: [
		    'excelHtml5',
		    'csvHtml5',
		    'pdfHtml5'
		]
	    } );
	})
    </script>

